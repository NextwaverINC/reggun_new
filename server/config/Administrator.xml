      </Connection>
      <NextwaverDB Connection="NextwaverDatabase" DatabaseName="GunBook" TableName="Page" />
      <Query>
        <SQL ID="Main">
          SELECT TOP 10 [ID] 
          ,[UserName] as [USERNAME]
          ,[Menu] as [MENU]
          ,[Process] as [PROCESS]
          ,Replace([Param],'''','') as [PARAM]
          ,[Result]  as [RESULT]
          ,[CreateDate]  as [CREATEDATE]
          FROM [GunRegBook].[dbo].[Logs]
